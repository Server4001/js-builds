{
    "extends": "react",
    "ecmaFeatures": {
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "block-scoped-var": 2,
        "comma-dangle": 2,
        "consistent-return": 2,
        "curly": 2,
        "dot-notation": 2,
        "eol-last": 0,
        "no-alert": 0,
        "no-constant-condition": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-eq-null": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-irregular-whitespace": 2,
        "no-lone-blocks": 2,
        "no-obj-calls": 2,
        "no-param-reassign": 2,
        "no-shadow": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-underscore-dangle": 0,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-var": 2,
        "quotes": 0,
        "space-before-function-paren": 0,
        "use-isnan": 2,
        "valid-typeof": 2
    },
    "globals": {
        "jQuery": true,
        "$": true
    }
}
